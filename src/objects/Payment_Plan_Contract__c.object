    <fields>
        <fullName>Total_Amount_Due__c</fullName>
        <externalId>false</externalId>
        <formula>(Amount_13__c  - IF( Received_13__c = true,  Amount_13__c , 0) )
+ (Amount_12__c  - IF( Received_12__c = true,  Amount_12__c , 0) )
+ (Amount_11__c  - IF( Received_11__c = true,  Amount_11__c , 0) )
+ (Amount_10__c  - IF( Received_10__c = true,  Amount_10__c , 0) )
+ (Amount_9__c  - IF( Received_9__c = true,  Amount_9__c , 0) )
+ (Amount_8__c  - IF( Received_8__c = true,  Amount_8__c , 0) )
+ (Amount_7__c  - IF( Received_7__c = true,  Amount_7__c , 0) )
+ (Amount_6__c  - IF( Received_6__c = true,  Amount_6__c , 0) )
+ (Amount_5__c  - IF( Received_5__c = true,  Amount_5__c , 0) )
+ (Amount_4__c  - IF( Received_4__c = true,  Amount_4__c , 0) )
+ (Amount_3__c  - IF( Received_3__c = true,  Amount_3__c , 0) )
+ (Amount_2__c  - IF( Received_2__c = true,  Amount_2__c , 0) )
+ (Amount_1__c  - IF( Received_1__c = true,  Amount_1__c , 0) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Amount Due</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
